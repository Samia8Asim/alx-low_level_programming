#include <stdio.h>
#include <stdlib.h>

/**
 * main -  program that adds positive numbers
 *
 * @argc: number of argument
 * @argv: array list of argument
 *
 * Return: 0 (Success) and 1 (Failuer)
 */

int main(int argc, char **argv)
{
	int sum_num;
	char *c;

	while (--argc)
	{
		for (c = argv[argc]; *c; c++)
		{
			if (*c < '0' || *C > '9')
				return (printf("Error\n"), 1);
			sum_num += atoi(argv[argc]);
		}
		printf("%d\n", sum_num);
		return (0);
	}
}
